req, res) {\n  try {\n    await usersModel.findByIdAndDelete(req.params.id)\n\n    return res.status(200).send({ msg: 'User excluído com Sucesso!' })\n  } catch (error) {\n    return res.status(400).send({ msg: 'ERRO: Tudo cagado!!' })\n  }\n}\n\nasync function updateOne(req, res) {\n  try {\n    await usersModel.findByIdAndUpdate(req.params.id, {\n      $set: {\n        name: req.body.name,\n        email: req.body.email,\n        password: md5(req.body.password, process.env.GLOBAL_SALTKEY),\n      },\n    })\n\n    return res.status(201).send({ msg: 'usuario Editado com sucesso!' })\n  } catch (error) {\n    return res.status(400).send({ msg: 'ERRO, tudo cagado!', error })\n  }\n}\n\nexport default { register, login, getAll, getById, deleteOne, updateOne }\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"bbfacffab076fd4cf8703340ecb73bb4f2781f4f","contentHash":"bf7a2347d7d31f2a815a9e4ec37e44c488c1322d5f8c82afcd66e6001e452a60"},"/home/gustavo/dev/git/curso-jest-rocketseat/src/userRoute.js":{"path":"/home/gustavo/dev/git/curso-jest-rocketseat/src/userRoute.js","statementMap":{"0":{"start":{"line":3,"column":0},"end":{"line":5,"column":3}},"1":{"start":{"line":7,"column":15},"end":{"line":7,"column":33}},"2":{"start":{"line":9,"column":16},"end":{"line":9,"column":48}},"3":{"start":{"line":11,"column":23},"end":{"line":11,"column":63}},"4":{"start":{"line":13,"column":24},"end":{"line":13,"column":64}},"5":{"start":{"line":15,"column":39},"end":{"line":15,"column":93}},"6":{"start":{"line":17,"column":14},"end":{"line":17,"column":37}},"7":{"start":{"line":18,"column":18},"end":{"line":18,"column":317}},"8":{"start":{"line":19,"column":0},"end":{"line":19,"column":28}}},"fnMap":{"0":{"name":"_interopRequireDefault","decl":{"start":{"line":15,"column":9},"end":{"line":15,"column":31}},"loc":{"start":{"line":15,"column":37},"end":{"line":15,"column":95}},"line":15}},"branchMap":{"0":{"loc":{"start":{"line":15,"column":46},"end":{"line":15,"colum