ry {\n    console.log('Entrou')\n\n    const { filename: image } = req.file\n\n    const [name] = image.split('.')\n    const filename = `${name}.jpg`\n\n    console.log(filename)\n\n    await productModel.create({\n      title: req.body.title,\n      description: req.body.description,\n      price: req.body.price,\n      image: filename,\n    })\n\n    return res.status(201).send({ message: 'Product Registered with success!' })\n  } catch (error) {\n    return res.status(201).send({ message: 'ERRO, tudo cagado', error })\n  }\n}\n\nasync function getAll(req, res) {\n  try {\n    const data = await productModel.find()\n\n    return res.status(200).send({ data })\n  } catch (error) {\n    return res.status(400).send(error)\n  }\n}\n\nasync function getById(req, res) {\n  try {\n    const data = await productModel.findByIdAndUpdate(req.params.id)\n\n    return res.status(200).send(data)\n  } catch (error) {\n    return res.status(400).send(error)\n  }\n}\n\nasync function updateOne(req, res) {\n  try {\n    if (typeof req.body.price === String) {\n      return alert('O preço deve ser um numero!')\n    } else {\n      await productModel.findByIdAndUpdate(req.params.id, {\n        $set: {\n          title: req.body.title,\n          description: req.body.description,\n          price: req.body.price,\n        },\n      })\n\n      return res\n        .status(201)\n        .send({ message: 'Product Registered with success!' })\n    }\n  } catch (error) {\n    return res.satus(400).send({ Menssagem: 'Tudo cagado!', error })\n  }\n}\n\nasync function deleteOne(req, res) {\n  try {\n    const { id } = req.params\n\n    const imagem = await productModel.findById(id)\n\n    console.log(imagem.image)\n\n    console.log('entrou')\n    await productModel.findByIdAndDelete(id)\n\n    fs.unlink(`uploads/${imagem.image}`, (err) => {\n      if (err) throw err\n      console.log('uploads/file.txt was deleted')\n    })\n\n    return res.status(201).send({ message: 'Product Deleted with success!' })\n  } catch (error) {\n    return res.satus(400).send({ Mensagem: 'All cagado!!', error })\n  }\n}\n\nexport default { getAll, create, deleteOne, updateOne, getById }\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"49b4a69e0cba3c6dd209732188b6beb5f9eaafbb","contentHash":"3b123507158a92d43a2991b660561804cbe9dc0582df15124d3b21c85f0c9ddf"},"/home/gustavo/dev/git/curso-jest-rocketseat/src/models/productModel.js":{"path":"/home/gustavo/dev/git/curso-jest-rocketseat/src/models/productModel.js","statementMap":{"0":{"start":{"line":3,"column":0},"end":{"line":5,"column":3}},"1":{"start":{"line":7,"column":16},"end":{"line":7,"column":35}},"2":{"start":{"line":9,"column":15},"end":{"line":17,"column":2}},"3":{"start":{"line":18,"column":0},"end":{"line":18,"column":63}}},"fnMap":{},"branchMap":{},"s":{"0":1,"1":1,"2":1,"3":1},"f":{},"b":{},"inputSourceMap":{"version":3,"sources":["productModel.js"],"names":["schema","Schema","title","type","String","unique","description","price","Number","image"],"mappings":";;;;;;AAAA;;AAEA,MAAMA,MAAM,GAAG,IAAIC,gBAAJ,CAAW;AACxBC,EAAAA,KAAK,EAAE;AACLC,IAAAA,IAAI,EAAEC,MADD;AAELC,IAAAA,MAAM,EAAE;AAFH,GADiB;AAKxBC,EAAAA,WAAW,EAAEF,MALW;AAMxBG,EAAAA,KAAK,EAAEC,MANiB;AAOxBC,EAAAA,KAAK,EAAEL;AAPiB,CAAX,CAAf;kBAUe,qBAAM,cAAN,EAAsBJ,MAAtB,C","sourceRoot":"/home/gustavo/dev/git/curso-jest-rocketseat/src/models/","sourcesContent":["import { model, Schema } from 'mongoose'\n\nconst schema = new Schema({\n  title: {\n    type: String,\n    unique: true,\n  },\n  description: String,\n  price: Number,\n  image: String,\n})\n\nexport default model('productModel', schema)\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"50d4f283b2b36132fc718c65d6e0858e85b15964","contentHash":"6809b253390186d8eab4958be9188a373442deb1f1117f19a0289860efbcc612"},"/home/gustavo/dev/git/curso-jest-rocketseat/src/controllers/usersController.js":{"path":"/home/gustavo/dev/git/curso-jest-rocketseat/src/controllers/u