  "dependencies": {
    "@babel/register": "^7.15.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-node": "^0.0.1-security",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "chai": "^4.3.4",
